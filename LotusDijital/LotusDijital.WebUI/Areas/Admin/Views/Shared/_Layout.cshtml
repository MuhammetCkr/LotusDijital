<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="Abstract" content="" />
    <meta name="Author" content="" />
    <title>@TempData["Title"]</title>
    <!--Kelimeler Max. 5 Kelime -->
    <script src="~/js/checkInput.js"></script>
    <meta name="keywords" content="" />

    <!-- Sayfa Açıklaması Max. 115 Karakter -->
    <meta name="description" content="" />
    <meta property="og:description" content="" />
    <meta itemprop="description" content="" />
    <meta name="twitter:description" content="" />
    @await Html.PartialAsync("_LinksPartial")
    @await RenderSectionAsync("Css", false)
    @await RenderSectionAsync("Links", false)

</head>
<body >
    <!--Container Main start-->
    <div class="wrapper">
        <!-- Sidebar  -->
        @await Html.PartialAsync("_LeftMenuPartial")
        <div id="content">
            <!-- Page Content  -->
            @RenderBody()
        </div>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
        </div>
    </footer>
    <!--Container Main end-->
    @await Html.PartialAsync("_ScriptsPartial")
    @await RenderSectionAsync("Scripts", false)
</body>
</html>

